<%= render "layouts/navbar" %>
<div id="user_nav">
  <% if current_user %>
    Logged in as <%= current_user.role %>.
    <%= link_to "Log out", log_out_path %>
  <% end %>
</div>
<% cache 'title_'+@board.abbreviation do %>
<div class="pagetitle">
  /<%= @board.abbreviation %>/ &ndash; <%= @board.title %>
  <hr />
</div>
<% end %>